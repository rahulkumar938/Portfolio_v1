
@{
    ViewBag.Title = "Education";
    Layout = "~/Views/Shared/_LayoutMin.cshtml";
}


<div>
    <br><br>
    <div id="" class="col-sm-offset-2 col-sm-8" style="padding:8px">
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                @*<li data-target="#myCarousel" data-slide-to="3"></li>
                <li data-target="#myCarousel" data-slide-to="3"></li>*@
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <img src="~/Documents/10th.jpg" alt="HSC" />
                    <div class="carousel-caption" style="color:white">
                        <h3>Higher Secondary</h3>
                        <p>CBSE, Delhi</p>
                    </div>
                </div>

                @*<div class="item">

                    <img src="~/Documents/10thcerti.jpg" alt="HSC certificate" />
                    <div class="carousel-caption" style="color:white">
                        <h3>Higher Secondary Certificate</h3>
                        <p>CBSE, Delhi</p>
                    </div>
                </div>*@

                <div class="item">
                    <img src="~/Documents/12th.jpg" alt="HSC Marksheet" />
                    <div class="carousel-caption" style="color:white">
                        <br />
                        <h3>Senior Secondary</h3>
                        <p>CBSE, Delhi</p>
                    </div>
                </div>

                @*<div class="item">
                    <img src="~/Documents/12certi.jpg" alt="12th Certificate"/>
                    <div class="carousel-caption" style="color:white">
                        <h3>Senior Secondary Certificate</h3>
                        <p>CBSE, Delhi</p>
                    </div>
                </div>*@

                <div class="item">
                    <img src="~/Documents/Engineering.jpg" alt="Engineering" />
                    <div class="carousel-caption" style="color:white">
                        <h3>Bacherlor Of Technology</h3>
                        <p>GGSIPU, Delhi</p>
                    </div>
                </div>
            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</div>
       
